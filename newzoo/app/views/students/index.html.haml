-#This file is app/views/movies/index.html.haml
%h2 All Students
<img src="/assets/zoo.png">
%table#students
  %thead
    %tr
      %th Name
      %th Grade
      %th Class
      %th Seatnumber
      %th Grouping
      %th Job
      %th Pet Number
      %th Pet Type
      %th Food
      
  %tbody
    - @students.each do |student|
      %tr
        %td= student.a 
        %td= student.b
        %td= student.c
        %td= student.d
        %td= student.e
        %td= student.f
        %td= student.g
        %td= student.h
        %td= student.i
        %td= link_to "More about #{student.a}", student_path(student)
-# add to end of index.html.haml

= link_to 'Add new student', new_student_path
